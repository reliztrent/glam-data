---
layout: none
---
{% assign date_list = '' %}

{% for file in site.data.gmdmar-facets-daily %}
  {% assign file_date = file[0] | slice: -8, 8 %}
  {% assign date_list = date_list | append: file_date %}
  {% if forloop.last == false %}
    {% assign date_list = date_list | append: ',' %}
  {% endif %}
{% endfor %}

{% assign date_array = date_list | split: ',' %}
{{ date_array }}
{% assign date_array_sorted = date_array | sort: date_list %}
{{ date_array_sorted }}
{% assign last_modified = date_array_sorted | first %}
{% assign year = last_modified | slice: 1,4 %}
{% assign month = last_modified | slice: 5,6 %}
{% assign day = last_modified | slice: 7,8 %}

{{ year }}
{{ month }}
{{ day }}