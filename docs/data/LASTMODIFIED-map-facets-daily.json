---
layout: none
---
{% assign date_list = '' %}

{% for file in site.data.gmdmar-facets-daily %}
  {% assign file_date = file[0] | slice: -8, 8 %}
  {% assign date_list = date_list | append: file_date %}
  {% if forloop.last == false %}
    {% assign date_list = date_list | append: ',' %}
  {% endif %}
{% endfor %}

{% assign date_list = date_list | split: ',' %}
{% assign date_list = date_list | sort: date_list %}
{% assign last_modified = date_list | first %}
{% assign year = last_modified | slide: 1,4 %}
{% assign month = last_modified | slide: 5,6 %}
{% assign day = last_modified | slide: 7,8 %}

{{ year }}
{{ month }}
{{ day }}