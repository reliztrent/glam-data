---
layout: none
---

{% assign filenames = '' | split: ',' %}
{% for file in site.data.gmdmar-facets-daily %}
	{% if file[0].size == 31 %}
	{% if file[1].pagination.of > 0 %}
	 {% assign filenames = filenames | push: file[0] %}
	{% endif %}
	{% endif %}
{% endfor %}
{% assign sorted_filenames  = filenames | sort | reverse %}


[{% for filename in sorted_filenames %}
	{% for file in site.data.gmdmar-facets-daily %}
		{% if file[0] == filename %}
		  {
		    "new": "{{ file[1].pagination.of}}",
		    "file": "{{ file[0] }}",
		    "results":[
		    {% for result in file[1].results. %} 
		     {
		     	"number_source_modified":"{{ result.number_source_modified[0] }}",
		     	"timestamp":"{{ result.timestamp }}",
		     	"extract_timestamp": "{{ result.extract_timestamp }}"
		     }{% if forloop.last == false %},{% endif %}
		    {% endfor %}
			]
		  }{% endif %}{% endfor %},
{% endfor %}
{}
]